Algoritmo Operaciones
	
	Escribir  "Ingrese el numero:"
	Leer num1
	Escribir  "Ingrese otro numero:"
	Leer num2
	
	Suma<-num1+num2
	Escribir "Resultado de la suma: ", Suma
	
	Resta<-num1-num2
	Escribir "Resultado de la Resta: ", Resta
	
	Multiplicación<-num1*num2
	Escribir "Resultado de la Multiplicación: ", Multiplicación
	
	División =num1+num2
	Escribir "Resultado de la División: ", División
	
	
FinAlgoritmo
